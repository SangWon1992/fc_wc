CREATE TABLE cont (
  cont_url long not null,
  cont_title long not null,
  cont_post long not null,
  cont_id int(255) not null,
  PRIMARY KEY (cont_id)
);

CREATE TABLE comm (
  comm_id int(255) not null,
  comm_cont long not null,
  cont_id int(255) not null,
  PRIMARY KEY (comm_id),
  FOREIGN KEY (cont_id) REFERENCES cont (cont_id)
);

